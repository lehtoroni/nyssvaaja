{
    "compilerOptions": {
        "baseUrl": ".",
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "h",
        "lib": ["dom", "esnext"],
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "target": "es6",
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "paths": {
          "react": ["./node_modules/preact/compat/"],
          "react-dom": ["./node_modules/preact/compat/"]
        },
    },
    "include": ["src/**/*"]
}